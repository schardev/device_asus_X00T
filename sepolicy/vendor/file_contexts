# APEX
/data/vendor/metadata_apex(/.*)?                                                                u:object_r:apex_metadata_file:s0
/data/vendor/metadata_apex/sessions(/.*)?                                                       u:object_r:apex_metadata_file:s0

# blutmac script
/(vendor|system/vendor)/bin/init\.btmac\.sh                                                     u:object_r:init_blutmac_exec:s0
/(system_ext|system/system_ext)/bin/nvram_btwifi                                                u:object_r:nvram_btwifi_exec:s0

# Fingerprint
/data/system/cdfinger(/.*)?                                                                     u:object_r:cdfinger_data_file:s0
/dev/fpsdev0                                                                                    u:object_r:cdfinger_device:s0
/dev/goodix_fp                                                                                  u:object_r:fingerprintd_device:s0
/(vendor|system/vendor)/bin/hw/android\.hardware\.biometrics\.fingerprint@2\.[1-2]-service-X00T u:object_r:hal_fingerprint_default_exec:s0

